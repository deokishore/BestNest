<script type="text/javascript">
$(document).ready(function() {
	$('#filter li a').click(function(e) {
		
		e.preventDefault();  
		
		// fetch the class of the clicked item
		var ourClass = $(this).attr('id');
		
		// reset the active class on all the buttons
		$('#filter a').removeClass('selected');
		
		// update the active state on our clicked button
		$(this).addClass('selected');
		
		$('#profilter').children('div.list-items').show();
		$('#portfolio li').removeClass('selected');
		
		if(ourClass == 'all') {
			
			// show all our items
			$('#portfolio').children('li.item').show();
		}
		
		else {
			// hide all elements that don't share ourClass
			$('#portfolio').children('li:not(.' + ourClass + ')').hide();
			// show all elements that do share ourClass
			$('#portfolio').children('li.' + ourClass).show();
		}
		return false;
	});
	
	$('#portfolio li').click(function() {
		
		// fetch the class of the clicked item
		var ourClass = $(this).attr('class');
		
		// reset the active class on all the buttons
		$('#portfolio li').removeClass('selected');
		
		// update the active state on our clicked button
		$(this).addClass('selected');
		
	});
	
		$('#portfolio li a').click(function(e){  
	
		//prevent the default behaviour of the link  
		e.preventDefault();  
	
		//get the id of the clicked link(which is equal to classes of our content  
		var filter = $(this).attr('id');  
		//alert(filter);
	
		//show all the list items(this is needed to get the hidden ones shown)  
		$('#profilter div.list-items').show();  
	
		/*using the :not attribute and the filter class in it we are selecting 
		only the list items that don't have that class and hide them '*/  
		$('#profilter div.list-items:not(.' + filter + ')').hide();  
	
	});  
});

</script>
